OPENAI_API_KEY=
OPENAI_BASE_URL=

NEO4J_URI=bolt://neo4j:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=replace_with_strong_neo4j_password
NEO4J_DATABASE=neo4j

QDRANT_HOST=qdrant
QDRANT_PORT=6333

ENVIRONMENT=docker
LOG_LEVEL=INFO

SECURITY__REQUIRE_ACCESS_TOKEN=true
SECURITY__AUTH_MODE=jwt
SECURITY__JWT_SECRET=replace_with_long_jwt_secret_min_32_chars
SECURITY__JWT_ALGORITHM=HS256
